{
  "name": "project_whiskey_echo_delta",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "aws-sdk": "^2.147.0",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.1",
    "react": "^16.0.0",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.0.0",
    "react-images": "^0.5.11",
    "react-loading": "^1.0.1",
    "react-masonry-component": "^6.0.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.14",
    "react-sizeme": "^2.3.6",
    "react-stack-grid": "^0.6.0"
  },
  "scripts": {
    "build-css":
      "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css":
      "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "prebuild":
      "aws s3 cp s3://${S3BUCKET}/aws-exports.js ./src/aws-exports.js",
    "build": "npm run build-css && react-scripts build",
    "predeploy": "npm run build",
    "deploy": "aws s3 cp build s3://${S3BUCKET} --recursive",
    "format": "prettier --write '**/*.{js,jsx}'",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
